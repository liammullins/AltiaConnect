/* QSlider style */

QSlider::groove:horizontal
{
    border: 1px solid #dedede;
    height: 16px; /* the groove expands to the size of the slider by default. by giving it a height, it has a fixed size */
    border: 1px solid #393939;
    background: #0a0a0a;
}
QSlider::handle:horizontal 
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:0, stop:0 #3fbeff, stop:1 #3fbeff);
    border: 1px outset #09a1ee;
    width: 42px;
    margin: 0px 0; /* handle is placed by default on the contents rect of the groove. Expand outside the groove */
    border-radius: 2px;
}
QSlider::groove:vertical 
{
    background: red;
    position: absolute; /* absolutely position 4px from the left and right of the widget. setting margins on the widget should work too... */
    left: 4px; 
    right: 4px;
}
QSlider::handle:vertical 
{
    height: 10px;
    background: green;
    margin: 0 -4px; /* expand outside the groove */
}
QSlider::add-page:vertical 
{
    background: black;
}

QSlider::sub-page:vertical 
{
    margin-top:50px;
    background: yellow;
}




