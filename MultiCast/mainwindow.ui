QTabWidget::pane 
{ /* The tab widget frame */
    border-top : none;
    border-left : none;
    border-bottom : 1px outset #555555;
    border-right : none;
    background-color:rgba(0, 0, 0, .70);
    
}

QTabWidget::tab-bar 
{
    left: 0px; /* move to the right by 5px */
    background-color:black;
}

QTabBar::tab
{
    /*
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #121212, stop: 0.4 #121212,
    stop: 0.5 #000, stop: 1.0 #000);
    border: 0px solid #cfcfcf;
    */
    background-color:rgba(0, 0, 0, .30);  
    border-bottom-color: #C2C7CB; /* same as the pane color */
    min-width: 150px;
    max-height:12px;
	min-height:12px;
    padding:12px;
    font-family: &quot;Consolas&quot;;
    font-size: 12px;
	font-weight:normal;
    color:#6b6b6b;
}

QTabBar::tab:selected
{
    /*
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #515151, stop: 0.4 #515151,
    stop: 0.5 #4e4e4e, stop: 1.0 #4e4e4e);  
    */
    
    background-color:rgba(0, 0, 0, .70);  
}

QTabBar::tab:hover 
{
    /*
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #272727, stop: 0.4 #272727,
    stop: 0.5 #272727, stop: 1.0 #272727);
   
    background-color:rgba(0, 0, 0, .50);   */
    border-bottom:1px inset #255681;
