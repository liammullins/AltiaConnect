QGraphicsView
{
    background: rgba(12,34,56,20);
    font-family : &quot;Consolas&quot;;
    border: 1px inset #272727;
    color : white;
    border-radius: 2px;
    font-size: 18px;
}


QGroupBox
{
	color:white;
	font-size:18px;
	/*border:1px solid #4f4f4f;*/
	border:none;
	border-radius: 3px;
    font-family : &quot;Consolas&quot;;
}

QTableWidget
{
    background : white;
    color      : black;
    
    outline    : none;
    
    
}
QTableWidget::item:focus
{
    background-color: qlineargradient(x1: 0, y1: 0.5, x2: 0, y2: 0.5,
    stop: 0 #17d9ff, stop: 1 white);
    border     : 1px solid #85f9ff;
    outline : none;
}

QLineEdit
{
    
    background: rgba(12,34,56,0);
    font-family : &quot;Consolas&quot;;
    padding-left: 7px;
    border: 1px inset #272727;
    color : white;
    border-radius: 1px;
    font-size: 18px;
    
}

QToolBox
{
    background: rgba(12,34,56,0);
    font-family : &quot;Consolas&quot;;
    padding: 15px;
    border: 1px inset #272727;
    color : white;
    border-radius: 5px;
    font-size: 16px;
    
}


QToolBox::tab 
{
    background:rgba(0, 0, 0, .78);
    border: 1px inset #272727;
    color : white;
    font-size: 20px;
    
}

QToolBox::tab:selected 
{
    background:rgba(0, 0, 0, .78);
    font-weight:bold;
    color:#cf0003;
    text-align:center;
}






/**
* Radiobutton style
*/
QRadioButton
{
    font-size: 16px;
    color : white;
    height: 32px;
}
QRadioButton::indicator::checked 
{
    image: url(style/images/radiobutton_red_active.png);
}
QRadioButton::indicator 
{
    image: url(style/images/radiobutton_red.png);
    color : #ffffff;
    font-size:16px;
}




QToolTip
{
    color: #e4f3ff;
    background-repeat:no-repeat;
    background-position:center;
    background-color:rgba(0, 0, 0, .40);
    border:none;
    border-radius:2px;
}

QMainWindow
{
    background-color:#000;
    border:0px inset #000;
    border-image: url(style/images/bg_blue_rust.jpg) no-repeat ;
    background-position: top center;
    font-family : &quot;Consolas&quot;;
}

/* Progress bar style */
QProgressBar 
{
    border: 1px inset #202020;
    border-radius: 5px;
}

QProgressBar::chunk
{
    background-color: #e51937;
    width: 20px;
}

/* Label - default style */
QLabel
{
    font-family:&quot;arial&quot;;
    font-size:16px;
    color:#ffffff;
}


/* Checkbox */
QCheckBox
{
    spacing: 5px;
    font-family: &quot;arial&quot;;
    font-size : 16px;
    padding-left : 5px;
    text-align: left;
    color : #e9e9e9;
}

QCheckBox::indicator 
{
    width: 40px;
    height: 38px;
    color : #e9e9e9;
}
QCheckBox::indicator:hover
{
    image: url(style/images/cbLB1.png);
    color : #e9e9e9;     
}
QCheckBox::indicator:unchecked
{
    image: url(style/images/cbLB1.png);
    color:#ffffff;
}

